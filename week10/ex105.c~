#include<stdio.h>
int main(){
  int i,j;
  int A[3][3],B[3][3],C[3][3];
  for(i=0;i<3;i++){
    for(j=0;j<3;j++){
      printf("A[%d][%d]: ",i+1,j+1);
      scanf("%d",&A[i][j]);
    }
  }
   for(i=0;i<3;i++){
    for(j=0;j<3;j++){
      printf("B[%d][%d]: ",i+1,j+1);
      scanf("%d",&B[i][j]);
    }
  }
   for(i=0;i<3;i++){
    for(j=0;j<3;j++){
     C[i][j]
    }
  }

  return 0;
}
